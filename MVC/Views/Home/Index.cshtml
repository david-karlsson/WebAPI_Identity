@{
    ViewData["Title"] = "Home Page";
}


<a asp-area="" asp-controller="Home" asp-action="Logout">Logga ut</a>

<h1>ÄRENDEN</h1>
<ul>
    @foreach (var c in ViewBag.Cases)
